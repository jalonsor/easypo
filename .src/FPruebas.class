Private $googTrans As GoolgleTranslator


Public Sub Form_Open()
  Dim i As Integer = 0
  
  $googTrans = New GoolgleTranslator
  
  '$googTrans.updateGoogleData
  
  For i = 0 To $googTrans.sourceLang.Count - 1
    ComboBox1.Add($googTrans.sourceLang[i])
  Next
  
  For i = 0 To $googTrans.targetLang.Count - 1
    ComboBox2.Add($googTrans.targetLang[i])
  Next
End

