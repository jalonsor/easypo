' Gambas class file


''http://ascii.cl/es/codigos-html.htm

Public Const IDX_SYMBOL As Integer = 0
Public Const IDX_HTML_NUMBER As Integer = 1
Public Const IDX_HTML_NAME As Integer = 2
Public Const IDX_DESCRIPTION As Integer = 3



Public Function getSymbol(strName As String) As String
	Dim strAux As String = "", strAuxName As String, nTam As Integer
	
	strAuxName = Trim(strName)
	
	If Left(strAuxName, 2) = "&#" And Right(strAuxName, 1) = ";" Then
		nTam = Len(strAuxName)
		strAux = Mid$(strAuxName, 3, nTam - 1 - 2)
		Return String.chr(CInt(strAux))
	Else If Left(strAuxName, 1) = "&" And Right(strAuxName, 1) = ";"
		If $asciiTB.Exist(strAuxName) Then
			Return $asciiTB[strAuxName][Me.IDX_SYMBOL]
		Endif
	Endif
	Return "(" & strAuxName & ": #N/A)"
End

Public Function getInfo(strName As String, Optional infoToReturn As Integer = Me.IDX_SYMBOL) As String
	Dim strAux As String = "", strAuxName As String, nTam As Integer
	
	strAuxName = Trim(strName)
	
	If Left(strAuxName, 2) = "&#" And Right(strAuxName, 1) = ";" Then
		nTam = Len(strAuxName)
		strAux = Mid$(strAuxName, 3, nTam - 1 - 2)
		If $asciiTB.Exist(strAux) Then
			Return $asciiTB[strAux][infoToReturn]
		Endif
	Else If Left(strAuxName, 1) = "&" And Right(strAuxName, 1) = ";"
		If $asciiTB.Exist(strAuxName) Then
			Return $asciiTB[strAuxName][infoToReturn]
		Endif
	Endif
	Return "(" & strAuxName & ": #N/A)"
End


